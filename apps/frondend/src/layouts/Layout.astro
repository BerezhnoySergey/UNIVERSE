---
interface Props {
 title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
 <head>
  <meta charset="UTF-8" />
  <meta name="description" content="Astro description" />
  <meta name="generator" content={Astro.generator} />
  <title>{title}</title>
 </head>
 <body class="bg-black">
  <slot />
 </body>
</html>